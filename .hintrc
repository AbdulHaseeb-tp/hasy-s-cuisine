{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "detect-css-reflows/layout": "off"
  }
}